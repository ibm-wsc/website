# Now define filesystems that are mounted at boot time #
########################################################
{% for file_system in file_systems %}
{%- if file_system['mount_point'] is defined %}
{%- if 'ROOT' in file_system['mount_point'] %}ROOT FILESYSTEM('{{file_system['dataset_name']}}')
{%- else %}
MOUNT FILESYSTEM('{{file_system['dataset_name']}}')
MOUNTPOINT('{{file_system['mount_point']}}')
{%- endif %}
{%- endif %}
{%- if file_system['fstype'] is defined %}
TYPE({{file_system['fstype']}})
{%- endif %}
{%- if file_system['mode'] is defined %} {%- if 'Read-Write' in file_system['mode'] %} MODE(RDWR){%- else %}
MODE(READ)
{%- endif %}
{%- if file_system['automove'] is defined %}
{%- if file_system['automove'] is false %}NOAUTOMOVE
{%- endif %} {%- endif %}
{% endif %}

{%- endfor %}
# Network Definitions
{% for network_data in network_definitions %}
FILESYSTYPE TYPE({{network_data['fstype']}}) ENTRYPOINT({{network_data['entry_point']}})
{%- for domain in network_data['networks'] %}
{%- if domain['domain_name'] is defined %}
{%- filter indent(width=2)%}
NETWORK DOMAIN({{domain['domain_name']}}) DOMAINNUMBER({{ domain['domain_number']}})
{%- endfilter %}
{%- endif %}
{%- if domain['maximum_number_of_sockets'] is defined %}
{%- filter indent(width=10) %}
MAXSOCKETS({{domain['maximum_number_of_sockets']}})
{%- endfilter %}
{%- endif %}
{%- if domain['fstype'] is defined %} TYPE({{domain['fstype']}}) {%- endif %}
{%- if domain['sub_filesystem'] is defined %}
{%- filter indent(width=2) %}
SUBFILESYSTYPE NAME({{domain['name']}}) TYPE({{domain['fstype']}}) ENTRYPOINT({{domain['entry_point']}})
{%- endfilter %}
{%- endif %}
{% endfor %}
